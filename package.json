{
	"publisher": "felipeguedes",
	"name": "json2java",
	"displayName": "json to java",
	"description": "transforms json to java class",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:json2java.convert"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "json2java.convert",
				"title": "Convert Java From JSON"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@semantic-release/github": "^7.0.7",
		"@types/glob": "^7.1.1",
		"@types/lodash.camelcase": "^4.3.6",
		"@types/lodash.upperfirst": "^4.3.6",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.45.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^10.2.0",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"release": {
		"branches": "master",
		"verifyConditions": [
			"@semantic-release/github"
		],
		"publish": [
			"@semantic-release/github"
		],
		"success": [
			"@semantic-release/github"
		],
		"fail": [
			"@semantic-release/github"
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/guedes4g/json-to-java.git"
	},
	"dependencies": {
		"esm": "^3.2.25",
		"lodash.camelcase": "^4.3.0",
		"lodash.upperfirst": "^4.3.1"
	}
}
